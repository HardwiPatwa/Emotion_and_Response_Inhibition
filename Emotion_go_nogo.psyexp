      <CodeComponent name="code_2" plugin="None">
        <Param val="import pandas as pd&amp;#10;import random&amp;#10;from psychopy import event&amp;#10;&amp;#10;df = pd.read_excel(&quot;condition.xlsx&quot;)&amp;#10;&amp;#10;num_trials= 60&amp;#10;num_go_trials = 45&amp;#10;num_stop_trials = 15&amp;#10;num_images_per_emotion = 20&amp;#10;trial_data = []" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="import * as pd from 'pandas';&amp;#10;import * as random from 'random';&amp;#10;import {event} from 'psychopy';&amp;#10;df = pd.read_excel(&quot;condition.xlsx&quot;);&amp;#10;num_trials = 60;&amp;#10;num_go_trials = 45;&amp;#10;num_stop_trials = 15;&amp;#10;num_images_per_emotion = 20;&amp;#10;trial_data = [];&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="stop_signal_trials = {}&amp;#10;trial_list = []&amp;#10;omission_errors = 0&amp;#10;commission_errors = 0&amp;#10;discrimination_errors = 0&amp;#10;trial_index = 0&amp;#10;win.callOnFlip(event.clearEvents, eventType='keyboard')  # clear events on next flip&amp;#10;for emo in [&quot;anger&quot;, &quot;disgust&quot;, &quot;neutral&quot;]:&amp;#10;    images = df[df[&quot;Emotion&quot;] == emo][&quot;ImageFile&quot;].tolist()  &amp;#10;    &amp;#10;    images = images[:20]  # Duplicate images if needed to reach 20&amp;#10;    random.shuffle(images)&amp;#10;&amp;#10;    go_images = images[:15]  # 15 &quot;Go&quot; trials&amp;#10;    stop_images = images[15:20]  # 5 &quot;Stop&quot; trials&amp;#10;&amp;#10;    # Create trials&amp;#10;    go_trials = [(emo, img, &quot;Go&quot;) for img in go_images]&amp;#10;    stop_trials = [(emo, img, &quot;Stop&quot;) for img in stop_images]&amp;#10;&amp;#10;    # Store trials&amp;#10;    trial_list.extend(go_trials + stop_trials)&amp;#10;&amp;#10;    # Mark Stop trials in dictionary&amp;#10;    for img in stop_images:&amp;#10;        stop_signal_trials[img] = True&amp;#10;&amp;#10;# Shuffle the entire trial list&amp;#10;random.shuffle(trial_list)&amp;#10;trial_list_final = trial_list * 4&amp;#10;random.shuffle(trial_list_final)" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="stop_signal_trials = {};&amp;#10;trial_list = [];&amp;#10;omission_errors = 0;&amp;#10;commission_errors = 0;&amp;#10;discrimination_errors = 0;&amp;#10;trial_index = 0;&amp;#10;psychoJS.window.callOnFlip(psychoJS.eventManager.clearEvents, {&quot;eventType&quot;: &quot;keyboard&quot;});&amp;#10;for (var emo, _pj_c = 0, _pj_a = [&quot;anger&quot;, &quot;disgust&quot;, &quot;neutral&quot;], _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    emo = _pj_a[_pj_c];&amp;#10;    images = df[(df[&quot;Emotion&quot;] === emo)][&quot;ImageFile&quot;].tolist();&amp;#10;    images = images.slice(0, 20);&amp;#10;    Math.random.shuffle(images);&amp;#10;    go_images = images.slice(0, 15);&amp;#10;    stop_images = images.slice(15, 20);&amp;#10;    go_trials = function () {&amp;#10;    var _pj_d = [], _pj_e = go_images;&amp;#10;    for (var _pj_f = 0, _pj_g = _pj_e.length; (_pj_f &lt; _pj_g); _pj_f += 1) {&amp;#10;        var img = _pj_e[_pj_f];&amp;#10;        _pj_d.push([emo, img, &quot;Go&quot;]);&amp;#10;    }&amp;#10;    return _pj_d;&amp;#10;}&amp;#10;.call(this);&amp;#10;    stop_trials = function () {&amp;#10;    var _pj_d = [], _pj_e = stop_images;&amp;#10;    for (var _pj_f = 0, _pj_g = _pj_e.length; (_pj_f &lt; _pj_g); _pj_f += 1) {&amp;#10;        var img = _pj_e[_pj_f];&amp;#10;        _pj_d.push([emo, img, &quot;Stop&quot;]);&amp;#10;    }&amp;#10;    return _pj_d;&amp;#10;}&amp;#10;.call(this);&amp;#10;    trial_list.concat((go_trials + stop_trials));&amp;#10;    for (var img, _pj_f = 0, _pj_d = stop_images, _pj_e = _pj_d.length; (_pj_f &lt; _pj_e); _pj_f += 1) {&amp;#10;        img = _pj_d[_pj_f];&amp;#10;        stop_signal_trials[img] = true;&amp;#10;    }&amp;#10;}&amp;#10;Math.random.shuffle(trial_list);&amp;#10;trial_list_final = (trial_list * 4);&amp;#10;Math.random.shuffle(trial_list_final);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="psychoJS.eventManager.clearEvents();&amp;#10;key_resp_clock = new util.Clock();&amp;#10;key_resp_clock.reset();&amp;#10;if ((trial_index &lt; trial_list_final.length)) {&amp;#10;    [selected_emo, emo_image, selected_trial_type] = trial_list_final[trial_index];&amp;#10;    image_2.setImage(emo_image);&amp;#10;} else {&amp;#10;    trials.finished = true;&amp;#10;}&amp;#10;if ((selected_trial_type === &quot;Stop&quot;)) {&amp;#10;    poly3.opacity = 1;&amp;#10;} else {&amp;#10;    poly3.opacity = 0;&amp;#10;}&amp;#10;if ((Math.random.random() &lt; 0.5)) {&amp;#10;    shade = &quot;blue&quot;;&amp;#10;} else {&amp;#10;    shade = &quot;red&quot;;&amp;#10;}&amp;#10;if ((poly3.opacity === 1)) {&amp;#10;    corr_key = null;&amp;#10;} else {&amp;#10;    if ((poly3.opacity === 0)) {&amp;#10;        if ((shade === &quot;blue&quot;)) {&amp;#10;            corr_key = &quot;n&quot;;&amp;#10;        } else {&amp;#10;            corr_key = &quot;x&quot;;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
